{
  "name": "@synxty/eden-vscode",
  "version": "0.6.0",
  "displayName": "Eden",
  "description": "Bring your code to paradise",
  "preview": true,
  "icon": "icon.png",
  "license": "MIT",
  "publisher": "synxty",
  "type": "module",
  "engines": {
    "vscode": "^1.79.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/synxty/core.git",
    "directory": "eden/visual-studio-code"
  },
  "keywords": [
