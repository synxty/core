{
  "name": "eden-vscode",
  "version": "0.0.7",
  "displayName": "Eden",
  "description": "Bring your code to paradise",
  "preview": true,
  "icon": "eden-icon-vscode-light.png",
  "license": "MIT",
  "publisher": "synxty",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
