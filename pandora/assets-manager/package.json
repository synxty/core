{
  "name": "@synxty/assets-manager",
  "description": "Manage Synxty assets like icons, logos and banners with Pandora's Assets Manager.",
  "version": "3.0.2",
  "license": "MIT",
  "type": "module",
  "types": "lib/index.d.ts",
  "exports": {
    "types": "./lib/index.d.ts",
    "import": "./lib/index.js",
    "require": "./lib/index.cjs",
    "node": "./lib/index.cjs",
    "default": "./lib/index.js"
  },
  "files": [
    "lib"
  ],
  "devDependencies": {
    "@synxty/tsconfig": "workspace:^",
    "@types/node": "^18.16.0",
    "@vitest/coverage-c8": "^0.30.1",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.30.1"
  },
  "dependencies": {
    "@synxty/brand-assets": "workspace:^",
    "@xmldom/xmldom": "^0.8.7",
    "sharp": "^0.32.0"
  },
  "scripts": {
    "build": "tsup",
    "lint:": "tsc",
    "test": "vitest --run --no-threads",
    "test:coverage": "vitest --run --coverage --no-threads ",
    "test:watch": "vitest --no-threads"
