{
  "name": "@synxty/brand-assets",
  "description": "Synxty assets repository with configurations for relevant platforms.",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "exports": {
    "./icons": {
      "types": "./lib/icons/index.d.ts",
      "default": "./lib/icons/index.js",
      "import": "./lib/icons/index.js",
      "node": "./lib/icons/index.cjs",
      "require": "./lib/icons/index.cjs"
    },
    "./config": {
      "types": "./lib/config/index.d.ts",
      "default": "./lib/config/index.js",
      "import": "./lib/config/index.js",
      "node": "./lib/config/index.cjs",
      "require": "./lib/config/index.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "icons": ["./lib/icons/index.d.ts"],
      "config": ["./lib/config/index.d.ts"]
    }
  },
  "devDependencies": {
    "@figma-export/cli": "^4.5.0",
    "@figma-export/output-components-as-es6": "^4.5.0",
    "@figma-export/transform-svg-with-svgo": "^4.5.0",
    "@figma-export/types": "^4.5.0",
    "@figma-export/utils": "^4.0.0",
    "@types/node": "^18.16.0",
    "dotenv": "^16.0.3",
    "tsup": "^6.7.0",
    "tsx": "^3.12.7",
    "typescript": "^5.0.4"
  },
  "scripts": {
    "build": "sh scripts/build.sh",
    "lint": "tsc"
  }
